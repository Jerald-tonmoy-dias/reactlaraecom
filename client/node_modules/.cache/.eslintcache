`).then((res) => {\r\n      if (res.data.status == 200) {\r\n        swal({\r\n          title: \"Success\",\r\n          text: res.data.message,\r\n          icon: \"success\",\r\n        });\r\n\r\n        thisClicked.closest(\"tr\").remove();\r\n      } else if (res.data.status == 404) {\r\n        swal({\r\n          title: \"Success\",\r\n          text: res.data.message,\r\n          icon: \"success\",\r\n        });\r\n\r\n        thisClicked.innerText = \"Delete\";\r\n      }\r\n    });\r\n  };\r\n\r\n  // table input\r\n  let viewCategory_HTML_Table = \"\";\r\n\r\n  if (loading) {\r\n    return <h2>Loading Category....</h2>;\r\n  } else {\r\n    viewCategory_HTML_Table = categorylist.map((item, index) => {\r\n      return (\r\n        <>\r\n          <tr key={index}>\r\n            <td>{item.id}</td>\r\n            <td>{item.name}</td>\r\n            <td>{item.slug}</td>\r\n            <td>{item.status}</td>\r\n            <td>\r\n              <Link\r\n                className=\"btn btn-success btn-sm\"\r\n                to={`edit-category/${item.id}`}\r\n              >\r\n                Edit\r\n              </Link>\r\n            </td>\r\n            <td>\r\n              <button\r\n                type=\"button\"\r\n                onClick={(e) => handleDelete(e, item.id)}\r\n                className=\"btn btn-danger btn-sm\"\r\n              >\r\n                Delete\r\n              </button>\r\n            </td>\r\n          </tr>\r\n        </>\r\n      );\r\n    });\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <div className=\"container px-4\">\r\n        <div className=\"card mt-4\">\r\n          <div className=\"card-header d-flex justify-content-between align-items-center\">\r\n            <h2 className=\"my-4\">Category List</h2>\r\n            <Link\r\n              to=\"/admin/add-category\"\r\n              className=\"btn btn-primary btn-sm px-4 text-capitalize\"\r\n            >\r\n              add category\r\n            </Link>\r\